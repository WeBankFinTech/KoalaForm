{
  "name": "koala-form",
  "version": "2.0.1",
  "description": "vue form helper",
  "author": "aringlai",
  "license": "MIT",
  "scripts": {
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "cypress:open": "cypress open",
    "build:core": "lerna exec --scope @koala-form/core tsc",
    "build:fes": "lerna exec --scope @koala-form/fes-plugin tsc",
    "build:element": "lerna exec --scope @koala-form/element-plugin tsc",
    "publish:core": "cd packages/core && npm publish",
    "publish:fes": "cd packages/fes-plugin && npm publish",
    "publish:element": "cd packages/element-plugin && npm publish",
    "publish": "npm-run-all build:core build:fes build:element publish:core publish:fes publish:element"
  },
  "dependencies": {
    "@fesjs/fes-design": "0.7.15",
    "less": "4.1.2",
    "lodash-es": "4.17.15",
    "vue": "^3.2.27",
    "ant-design-vue": "^3.2.20",
    "element-plus": "^2.3.7",
    "dayjs": "^1.11.9",
    "@koala-form/core": "^2.0.0-rc.8",
    "@koala-form/fes-plugin": "^2.0.0",
    "@koala-form/element-plugin": "^2.0.0",
    "@koala-form/antd-plugin": "^2.0.0"
  },
  "workspaces": [
    "packages/*"
  ],
  "private": true,
  "devDependencies": {
    "@babel/core": "7.22.1",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "7.15.0",
    "@babel/preset-typescript": "^7.16.7",
    "@betit/rollup-plugin-rename-extensions": "^0.1.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@types/lodash-es": "4.17.5",
    "@vitejs/plugin-vue-jsx": "1.3.0",
    "@vue/repl": "0.4.4",
    "@vue/theme": "0.1.16",
    "@webank/eslint-config-ts": "1.0.0",
    "@webank/eslint-config-webank": "1.1.1",
    "eslint-plugin-react": "7.27.1",
    "fs-extra": "^10.0.0",
    "lerna": "6.6.2",
    "rollup": "^2.68.0",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "typescript": "4.5.2",
    "vitepress": "0.21.6",
    "cypress": "12.13.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "npm-run-all": "4.1.5"
  }
}
